@incollection{nagourney_frequency_2014,
	title = {Frequency and amplitude modulation},
	isbn = {978-0-19-966548-8},
	url = {https://doi.org/10.1093/acprof:oso/9780199665488.003.0015},
	abstract = {Many of the applications described in this book require the ability to modulate the frequency, phase or amplitude of laser radiation. The devices used for these purposes fall into two broad categories: those that exploit the dependence of the indices of refraction of certain crystals on an applied electric field (the electro-optic effect), and those that use an acoustic wave to modulate the index of refraction (via the acousto-optic effect). This chapter describes both categories of modulator and the underlying processes that enable them to function. Traveling-wave electro-optic modulators, which provide efficient modulation over a very wide range of modulation frequencies, are described. Acousto-optic modulators operating in both the Raman–Nath and Bragg regime are analyzed.},
