	pages = {012307},
	file = {APS Snapshot:/home/webb/Zotero/storage/97IWNSUH/PhysRevA.77.html:text/html;Full Text PDF:/home/webb/Zotero/storage/GFWZ2MBK/Knill et al. - 2008 - Randomized benchmarking of quantum gates.pdf:application/pdf},
}

