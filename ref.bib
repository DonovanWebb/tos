@incollection{nagourney_laser_2014,
	title = {Laser gain and some of its consequences},
	isbn = {978-0-19-966548-8},
	url = {https://doi.org/10.1093/acprof:oso/9780199665488.003.0005},
	abstract = {The laser gain equation is derived in two ways. First, the optical Bloch equations are derived, based upon a semiclassical treatment of a two-level atomic system and a radiation field. As a visualization aid, there follows a discussion of the formally identical classical Bloch equations, which describe the behavior of a magnetic moment in a combined static and rotating magnetic field. It is shown that laser gain is proportional to the imaginary part of the linear susceptibility, which is obtained from the semiclassical treatment. The second viewpoint uses a slightly modified form of Einstein’s A and B coefficients adapted to describing interactions between atoms and monochromatic fields. Various damping and broadening mechanisms are introduced in a reasonable but non-rigorous fashion, and the crucial distinction between homogeneous and inhomogeneous broadening is introduced.},
